@article{jerde_etal2011Sightunseendetection,
  title = {``{{Sight-unseen}}'' Detection of Rare Aquatic Species Using Environmental {{DNA}}: {{eDNA}} Surveillance of Rare Aquatic Species},
  shorttitle = {``{{Sight-unseen}}'' Detection of Rare Aquatic Species Using Environmental {{DNA}}},
